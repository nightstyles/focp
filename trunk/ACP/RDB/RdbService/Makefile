ifeq ($(FOCP_VOB_DIR),)
	FOCP_VOB_DIR=/vob
endif

FOCP_MODULE_NAME=RdbService
FOCP_TARGET_TYPE=DLL

FOCP_INCLUDE_LIST+=$(FOCP_VOB_DIR)/AFW/AFC $(FOCP_VOB_DIR)/ACP/RDB/VmmApi/inc $(FOCP_VOB_DIR)/ACP/RDB/RdbApi/inc $(FOCP_VOB_DIR)/ACP/RDB/SqlApi/inc
FOCP_SOURCE_LIST+=$(FOCP_VOB_DIR)/ACP/RDB/RdbService

FOCP_LIB_LIST+=VmmApi RdbApi SqlApi

FOCP_LIB_HOME=$(FOCP_APP_HOME)/lib/Services
FOCP_LIBDIR_LIST+=$(FOCP_APP_HOME)/lib

ifneq ($(RDB_SUPPORT_MEMORY_DB),)
	FOCP_MACRO_DEFINES+=RDB_SUPPORT_MEMORY_DB
endif
ifneq ($(RDB_SUPPORT_DISK_DB),)
	FOCP_MACRO_DEFINES+=RDB_SUPPORT_DISK_DB
endif

include $(FOCP_VOB_DIR)/AFW/AFC/AfcLib.incl
include $(FOCP_VOB_DIR)/MakeOption/option.incl
